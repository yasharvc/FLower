{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AuthenticationSettings": {
    "AllowRefresh": false,
    "IsPersistent": false,
    "ExpireMinutes": 10,
    "Issuer": "http://www.flower.com/",
    "LogoutPath": "/Security/Logout",
    "LoginPath": "/Security/Login",
    "AccessDeniedPath":"/Security/Forbidden"
  },
  "AllowedHosts": "*"
}
